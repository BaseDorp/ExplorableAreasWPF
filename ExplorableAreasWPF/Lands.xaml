      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExplorableAreasWPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Lands">

    <Grid>
        <Image x:Name="img_Background" HorizontalAlignment="Left" Height="450" VerticalAlignment="Top" Width="800" OpacityMask="White" Source="Beach.png"/>
        <Button x:Name="bt_Dous" Content="Mountain" HorizontalAlignment="Left" Margin="669,380,0,0" VerticalAlignment="Top" Width="83" Height="60" Click="bt_Dous_Click"/>
        <Button x:Name="bt_Uno" Content="Lake" HorizontalAlignment="Left" Margin="511,380,0,0" VerticalAlignment="Top" Width="83" Height="60" Click="bt_Uno_Click"/>

    </Grid>
</Page>
